/*公共类*/
@mixin opacity ($val) {
    opacity: $val;
    -moz-opacity: $val;
    -webkit-opacity: $val;
}
@mixin backgroundsize($x, $y){
    background-size: $x $y;
    -webkit-background-size: $x $y;
}
@mixin ellipsis{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
@mixin radius($val){
    border-radius: $val;
    -moz-border-radius: $val;
    -webkit-border-radius: $val;
    -o-border-radius: $val;
}
@mixin arrow($width,$height,$color){
    width: $width;
    height: $height;
    border-top: 0.1rem solid $color;
    border-right: 0.1rem solid $color;
    display: block;
}
@mixin routate($val){
    transform: rotate($val);
    -moz-transform: rotate($val);
    -webkit-transform: rotate($val);
    -o-transform: rotate($val);
}
/*start*/
html{
    -ms-touch-action: none;
}
body{
    overflow-x: hidden;
    width: 100%;
    font-size: 1.4rem;
    background: #222538;
    color: #727182;
}
.dis-flex {
    display: -moz-box;
    display: -ms-box;
    display: -webkit-box;
    display: box;
    overflow: hidden;
    .dis-flex-1 {
        display: block;
        -moz-box-flex: 1.0;
        -webkit-box-flex: 1.0;
        box-flex: 1.0;
    }
}
#pageLoader{
    position: fixed;
    left: 50%;
    top: 50%;
    z-index: 9000;
    overflow: hidden;
    background: #fff;
    @include radius(100%);
    text-align: center;
    &.hide{
        display: block !important;
        opacity: 0;
        transform: scale(0);
        -webkit-transform: scale(0);
        transition: all 0.4s ease-in-out;
        -webkit-transition: all 0.4s ease-in-out;
        .loader{
            opacity: 0;
        }
    }
    &.show{
        opacity: 1;
        transform: scale(1);
        -webkit-transform: scale(1);
        transition: all 0.4s ease-in-out;
        -webkit-transition: all 0.4s ease-in-out;
        .loader{
            opacity: 1;
            transition: all 0.1s 0.4s ease-in-out;
            -webkit-transition: all 0.1s 0.4s ease-in-out;
        }
        .loading-1, 
        .loading-3{
            animation: loading1 linear 0.5s infinite alternate;
            -webkit-animation: loading1 linear 0.5s infinite alternate;
        }
        .loading-2{
            animation: loading2 linear 0.5s infinite alternate;
            -webkit-animation: loading2 linear 0.5s infinite alternate;
        }
    }
    .loader{
        position: absolute;
        opacity: 0;
        left: 50%;
        top: 50%;
        margin-top: -1rem;
        margin-left: -5rem;
        z-index: 9001;
        height: 2rem;
        width: 10rem;
        text-align: center;
        span{
            display: inline-block;
            width: 2rem;
            height: 2rem;
            @include radius(2rem);
            &.loading-1{
                background: #3ae684;
            }
            &.loading-2{
                background: #5688dd;
                margin: 0 1rem;
            }
            &.loading-3{
                background: #3ae684;
            }
            @keyframes loading1{
                0%{
                    opacity: 1;
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
                100%{
                    opacity: 0.4;
                    transform: scale(0.6);
                    -webkit-transform: scale(0.6);
                }
            }
            @-webkit-keyframes loading1{
                0%{
                    opacity: 1;
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
                100%{
                    opacity: 0.4;
                    transform: scale(0.6);
                    -webkit-transform: scale(0.6);
                }
            }
            @keyframes loading2{
                0%{
                    opacity: 0.4;
                    transform: scale(0.6);
                    -webkit-transform: scale(0.6);
                }
                100%{
                    opacity: 1;
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
            }
            @-webkit-keyframes loading2{
                0%{
                    opacity: 0.4;
                    transform: scale(0.6);
                    -webkit-transform: scale(0.6);
                }
                100%{
                    opacity: 1;
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
            }
        }
    }
}
/*提示*/
#tips {
    position: fixed;
    left: 0;
    top: 50%;
    z-index: 1000;
    margin-top: -3rem;
    text-align: center;
    z-index: 2000;
    width: 100%;
    height: 6rem;
    opacity: 1;
    &.ng-hide-remove{
        transform: scale(1);
        -webkit-transform: scale(1);
        opacity: 1;
        transition: all 0.3s ease-in-out;
        -webkit-transition: all 0.3s ease-in-out;
    }
    &.ng-hide{
        transform: scale(2);
        -webkit-transform: scale(2);
        opacity: 0;
        transition: all 0.3s ease-in-out;
        -webkit-transition: all 0.3s ease-in-out;
    }
    span{
        position: relative;
        padding: 1rem;
        height: 6rem;
        line-height: 4rem;
        border-radius: 4px;
        background: rgba(0, 0, 0, 0.8);
        color: #fff;
    }
}
/*公共确认框*/
#js-confirm{
    position: fixed;
    left: 50%;
    top: 50%;
    margin-top: -5rem;
    width: 80%;
    margin-left: -40%;
    background: #fff;
    z-index: 1001;
    text-align: center;
    transition: top 0.5s ease;
    -webkit-transition: top 0.5s ease;
    &.ng-animate.ng-enter{
        top: -20%;
    }
    &.ng-animate.ng-enter-active,
    &.ng-animate.ng-leave{
        top: 50%;
    }
    &.ng-animate.ng-leave-active{
        top: -20%;
    }
    .t{
        padding: 1.5rem 0;
        font-size: 1.4rem;
        border-bottom: 1px solid #eee;
    }
    .c{
        padding: 1rem 0;
        .l{
            width: 50%;
            border-right: 1px solid #eee;
            color: #01b4d2;
        }
        .r{
            width: 50%;
        }
    }
}
/*公共背景*/
#js-bg{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.2);
    z-index: 1000;
    opacity: 1;
    transition: opacity 0.5s ease-in-out;
    -webkit-transition: opacity 0.5s ease-in-out;
    &.ng-animate.ng-enter{
        opacity: 0;
    }
    &.ng-animate.ng-enter-active{
        opacity: 1;
    }
    &.ng-animate.ng-leave{
        opacity: 1;
    }
    &.ng-animate.ng-leave-active{
        opacity: 0;
    }
}
/*
 * loading
 */
#loading{
    position: fixed;
    top: 50%;
    left: 50%;
    width: 4rem;
    height: 4rem;
    padding: 0.5rem;
    margin-left: -2rem;
    margin-top: -2rem;
    border-radius: 0.5rem;
    z-index: 1010;
    text-align: center;
    background: rgba(0,0,0,0.8);
    img{
        display: inline-block;
        width: 3rem;
        height: 3rem;
    }
}
#max{
    max-width: 60rem;
    margin: 0 auto;
    display: none;
}
.nodate{
    width: 100%;
    padding: 7rem 0 3rem 0;
    background: url(../../images/wx/bg2.png) repeat center;
    @include backgroundsize(80%,80%);
    .ball{
        position: relative;
        margin: 0 auto;
        width: 15rem;
        height: 15rem;
        background: url(../../images/wx/bg3.png) no-repeat center;
        @include backgroundsize(100%,100%);
    }
    .move{
        position: absolute;
        left: 5rem;
        top: -1rem;
        z-index: 1;
        width: 12rem;
        animation: rocket ease-in-out 1s 1s;
        -webkit-animation: rocket ease-in-out 1s 1s;
        transform: scale(0.7);
        -webkit-transform: scale(0.7);
        @keyframes rocket{
            0%{
                left: -5rem;
                top: 8rem;
                transform: scale(1);
                -webkit-transform: scale(1);
            }
            100%{
                left: 5rem;
                top: -1rem;
                transform: scale(0.7);
                -webkit-transform: scale(0.7);
            }
        }
        @-webkit-keyframes rocket{
            0%{
                left: -5rem;
                top: 8rem;
                transform: scale(1);
                -webkit-transform: scale(1);
            }
            100%{
                left: 5rem;
                top: -1rem;
                transform: scale(0.7);
                -webkit-transform: scale(0.7);
            }
        }
        .rocket{
            position: relative;
            z-index: 3;
            float: right;
            width: 10rem;
        }
        .air{
            position: relative;
            z-index: 1;
            float: left;
            margin-top: -3rem;
            width: 5rem;
            animation: air ease-in-out 1s 1s infinite;
            -webkit-animation: air ease-in-out 1s 1s infinite;
            @keyframes air{
                0%{
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
                20%{
                    transform: scale(1.5);
                    -webkit-transform: scale(1.5);
                }
                50%{
                    transform: scale(1.2);
                    -webkit-transform: scale(1.2);
                }
                80%{
                    transform: scale(1.3);
                    -webkit-transform: scale(1.3);
                }
                100%{
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
            }
            @-webkit-keyframes air{
                0%{
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
                20%{
                    transform: scale(1.5);
                    -webkit-transform: scale(1.5);
                }
                50%{
                    transform: scale(1.2);
                    -webkit-transform: scale(1.2);
                }
                80%{
                    transform: scale(1.3);
                    -webkit-transform: scale(1.3);
                }
                100%{
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
            }
        }
    }
    .title{
        margin-top: 2rem;
        font-weight: 100;
        color: #fff;
        text-align: center;
    }
}
/* =======
 * 头部
 * =======
 */
.header{
    padding: 0 4rem;
    height: 6rem;
    line-height: 6rem;
    border-bottom: 0.1rem solid rgba(255,255,255,0.2);
    overflow: hidden;
    .date{
        display: block;
        float: left;
        margin-left: -4rem;
        padding-left: 1rem;
        width: 4rem;
        font-size: 2.2rem;
    }
    .title{
        float: left;
        width: 100%;
        text-align: center;
        font-size: 1.6rem;
    }
    .user{
        display: block;
        float: right;
        margin-right: -4rem;
        padding-right: 1rem;
        width: 4rem;
        text-align: right;
        font-size: 2.2rem;
    }
}
.calendar{
    position: relative;
    padding: 0 2rem;
    width: 100%;
    height: 5rem;
    text-align: center;
    border-top: 0.1rem solid rgba(255,255,255,0.2);
    border-bottom: 0.1rem solid rgba(255,255,255,0.2);
    background: #2e2f42;
    .prev{
        float: left;
        margin-left: -2rem;
        width: 2rem;
        text-align: center;
        height: 5rem;
        line-height: 5rem;
        color: #fff;
    }
    .list{
        float: left;
        width: 100%;
        height: 5rem;
        overflow: hidden;
        .item{
            position: relative;
            padding: 0.5rem 0;
            height: 5rem;
            line-height: 2rem;
            border-left: 0.1rem solid rgba(255,255,255,0.2);
            h4{
                font-size: 1.2rem;
                font-weight: 100;
                color: rgba(255,255,255,0.4);
            }
            p{
                color: #fff;
            }
            &.current{
                background: rgba(255,255,255,0.1);
            }
            &:nth-child(1){
                transition: padding-top 0.3s 0.2s ease-in-out;
                -webkit-transition: padding-top 0.3s 0.2s ease-in-out;
            }
            &:nth-child(2){
                transition: padding-top 0.3s 0.4s ease-in-out;
                -webkit-transition: padding-top 0.3s 0.4s ease-in-out;
            }
            &:nth-child(3){
                transition: padding-top 0.4s 0.8s ease-in-out;
                -webkit-transition: padding-top 0.3s 0.8s ease-in-out;
            }
            &:nth-child(4){
                transition: padding-top 0.3s 1.0s ease-in-out;
                -webkit-transition: padding-top 0.3s 1.0s ease-in-out;
            }
            &:nth-child(5){
                transition: padding-top 0.3s 1.2s ease-in-out;
                -webkit-transition: padding-top 0.3s 1.2s ease-in-out;
            }
            &:nth-child(6){
                transition: padding-top 0.3s 1.4s ease-in-out;
                -webkit-transition: padding-top 0.3s 1.4s ease-in-out;
            }
            &:nth-child(7){
                transition: padding-top 0.3s 1.6s ease-in-out;
                -webkit-transition: padding-top 0.3s 1.6s ease-in-out;
            }
            &.ng-enter{
                padding-top: 6rem;
            }
            &.ng-enter.ng-enter-active{
                padding-top: 0.5rem;
            }
        }
    }
    .next{
        float: right;
        margin-right: -2rem;
        width: 2rem;
        height: 5rem;
        line-height: 5rem;
        border-left: 0.1rem solid rgba(255,255,255,0.2);
        color: #fff;
        &.disable{
            color: rgba(255,255,255,0.1);
        }
    }
}
/* =======
 * 视图
 * =======
 */
.footer{
    padding: 1rem;
    text-align: center;
    .link{
        a{
            line-height: 4rem;
            color: #f36b11;
            text-decoration: underline;
        }
    }
    p{
        line-height: 2rem;
    }
}
/* =======
 * 登录
 * =======
 */
#login{
    position: relative;
    padding: 0 1rem;
    width: 100%;
    height: 100%;
    overflow: hidden;
    background: url(../../images/wx/loginbg.png) no-repeat center 20%;
    @include backgroundsize(100%,auto);
    .login{
        position: absolute;
        left: 15%;
        top: 50%;
        z-index: 2;
        margin-top: -10rem;
        width: 70%;
        height: 20rem;
        .title{
            width: 100%;
            height: 2rem;
            line-height: 2rem;
            color: #fff;
            font-weight: 100;
        }
        .text{
            display: block;
            width: 100%;
            margin-top: 1rem;
            height: 5rem;
            padding: 1rem;
            line-height: 3rem;
            border: 0;
            background: #fff;
            @include radius(0.3rem);
            font-size: 1.6rem;
            text-align: center;
            &.btn{
                background: #5688dd;
                color: #fff;
                &.loading{
                    animation: loginBtn 1.2s ease-in-out;
                    -webkit-animation: loginBtn 1.2s ease-in-out;
                    @keyframes loginBtn{
                        0%{
                            -webkit-transform: perspective(400px) rotateX(90deg);
                        }
                        20%{
                            -webkit-transform:perspective(400px) rotateX(-40deg);
                        }
                        50%{
                            -webkit-transform:perspective(400px) rotateX(20deg);
                        }
                        70%{
                            -webkit-transform:perspective(400px) rotateX(-10deg);
                        }
                        90%{
                            -webkit-transform:perspective(400px) rotateX(10deg);
                        }
                        100%{
                            -webkit-transform:perspective(400px) rotateX(0deg);
                        }
                    }
                    @-webkit-keyframes loginBtn{
                        0%{
                            -webkit-transform: perspective(400px) rotateX(90deg);
                        }
                        20%{
                            -webkit-transform:perspective(400px) rotateX(-40deg);
                        }
                        50%{
                            -webkit-transform:perspective(400px) rotateX(20deg);
                        }
                        70%{
                            -webkit-transform:perspective(400px) rotateX(-10deg);
                        }
                        90%{
                            -webkit-transform:perspective(400px) rotateX(10deg);
                        }
                        100%{
                            -webkit-transform:perspective(400px) rotateX(0deg);
                        }
                    }
                }
            }
        }
    }
    .bg{
        position: absolute;
        bottom: -10%;
        left: -10%;
        z-index: 1;
        width: 120%;
        height: 30%;
        background: #5688dd;
        border-top-left-radius: 120%;
        border-top-right-radius: 120%;
        -webkit-border-top-left-radius: 120%;
        -webkit-border-top-right-radius: 120%;
        overflow: hidden;
        .tips{
            position: relative;
            z-index: 10;
            top: 20%;
            text-align: center;
            color: #fda27a;
        }
        .bg-1{
            position: absolute;
            top: 20%;
            left: 12%;
            width: 30%;
            height: 20%;
            @include radius(100%);
            background: #5083d8;
            box-shadow: 0.1rem 0.1rem 0.3rem #396abb inset;
            transform: rotate(-15deg);
            -webkit-transform: rotate(-15deg);
        }
        .bg-2{
            position: absolute;
            top: 15%;
            right: 15%;
            width: 20%;
            height: 15%;
            @include radius(100%);
            background: #5083d8;
            box-shadow: -0.1rem 0.1rem 0.3rem #396abb inset;
            transform: rotate(13deg);
            -webkit-transform: rotate(13deg);
        }
        .bg-3{
            position: absolute;
            bottom: 26%;
            left: 45%;
            width: 30%;
            height: 20%;
            @include radius(100%);
            background: #5083d8;
