}
/* =======
 * 首页
 * =======
 */
#index{
    position: relative;
    z-index: 10;
    transition: padding-top 0.5s;
    -webkit-transition: padding-top 0.5s;
    .total{
        padding: 1rem;
        border-bottom: 0.1rem solid rgba(255,255,255,0.2);
        .t{
            height: 3rem;
            line-height: 3rem;
            overflow: hidden;
            color: #fff;
            i.fl{
                margin-top: 0.5rem;
                margin-right: 0.5rem;
                display: block;
                border: 1px solid #fff;
                width: 2rem;
                height: 2rem;
                @include radius(2rem);
                text-align: center;
                line-height: 2rem;
                font-size: 1.2rem;
            }
            span.fr{
                font-size: 1.2rem;
                line-height: 3.2rem;
                color: rgba(255,255,255,0.5);
            }
        }
        .c{
            position: relative;
            height: 10rem;
            text-align: center;
            a{
                display: block;
                width: 100%;
                height: 10rem;
                line-height: 10rem;
                overflow: hidden;
                strong{
                    position: relative;
                    z-index: 10;
                    font-weight: 100;
                    color: #3ae684;
                    font-size: 4rem;
                }
                i{
                    position: absolute;
                    right: 0;
                    top: 0;
                    height: 10rem;
                    line-height: 10rem;
                    z-index: 1;
                    color: #3ae684;
                    font-size: 2rem;
                }
            }
        }
        .b{
            width: 100%;
            height: 3rem;
            line-height: 3rem;
            overflow: hidden;
            text-align: center;
            color: #fff;
            span.fl,
            span.fr{
                display: inline-block;
                width: 50%;
                .up{
                    position: relative;
                    color: #f00;
                    margin-right: 0.5rem;
                    animation: up 0.5s infinite alternate;
                    -webkit-animation: up 0.5s infinite alternate;
                }
                @keyframes up{
                    0%{
                        top: -0.5rem;
                    }
                    100%{
                        top: 0.5rem;
                    }
                }
                @-webkit-keyframes up{
                    0%{
                        top: -0.5rem;
                    }
                    100%{
                        top: 0.5rem;
                    }
                }
                .down{
                    position: relative;
                    color: #00A000;
                    margin-right: 0.5rem;
                    animation: down 0.5s 0.2s infinite alternate;
                    -webkit-animation: down 0.5s 0.2s infinite alternate;
                }
                @keyframes down{
                    0%{
                        top: -0.5rem;
                    }
                    100%{
                        top: 0.5rem;
                    }
                }
                @-webkit-keyframes down{
                    0%{
                        top: -0.5rem;
                    }
                    100%{
                        top: 0.5rem;
                    }
                }
            }
        }
    }
    .box{
        border-bottom: 1px solid #45414f;
        div.fl,
        div.fr{
            width: 50%;
            padding: 1rem;
            color: #fff;
            @include radius(0.3rem);
            h4{
                font-size: 1.6rem;
                vertical-align: bottom;
                line-height: 4rem;
                font-weight: 100;
                color: rgba(255,255,255,0.6);
                i{
                    float: left;
                    margin-right: 0.5rem;
                    font-size: 3rem;
                }
            }
            p{
                padding-left: 0.2rem;
                height: 3rem;
                line-height: 3rem;
                color: rgba(255,255,255,0.6);
                em{
                    font-size: 2rem;
                    margin: 0 0.5rem;
                }
            }
        }
        div.fl{
            border-right: 1px solid #45414f;
            i,
            em{
                color: #cb5a5e;
            }
        }
        div.fr{
            border-right: 1px solid #45414f;
            i,
            em{
                color: #3ae684;
            }
        }
    }
    .line{
        padding: 1rem;
        width: 100%;
        overflow: hidden;
        background: #2e2f42;
        h4{
            font-size: 1.6rem;
            font-weight: 100;
            line-height: 3rem;
            color: rgba(255,255,255,0.6);
            i{
                float: left;
                font-size: 3rem;
                color: rgba(255,255,255,0.2);
                margin-right: 0.5rem;
            }
        }
        .canvas{
            margin-top: 1rem;
            width: 100%;
            height: 20rem;
        }
        .canvas-week{
            text-align: right;
        }
    }
}

