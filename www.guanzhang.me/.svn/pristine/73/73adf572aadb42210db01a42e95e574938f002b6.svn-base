.main{
  text-align: center;
  font-size: 18px;
  font-weight: 200;
  letter-spacing: 0.05em;
}

h2{
  margin-top: 215px;
  font-size: 40px;
  color:#2b6ffe;
  margin-bottom: 15px;
  font-weight: 300;
  letter-spacing: 0.05em;
}

.cb{
  margin: 40px auto;
}

.cell{
  display: inline-block;
  margin: 0 60px;
}

.li{
  color:#2b6ffe;
  list-style: circle;
  margin-bottom: 44px;
  position: relative;
  display: inline-block;
  margin-top: 5px;
  &:after{
    content: "";
    width: 5px;
    height: 5px;
    border-radius: 50%;
    background-color: #9e9e9e;
    position: absolute;
    left:-15px;
    top:10px;
  }
}