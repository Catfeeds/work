/*产品选择*/
.sel-goods{
  width: 100%;
  background: white;
  position: absolute;
  bottom: 0;
  left: 0;
  -webkit-transform: translateY(110%);
  -ms-transform: translateY(110%);
  -o-transform: translateY(110%);
  transform: translateY(110%);
  -webkit-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;

}
.sel-goods .have-sel{
  border-bottom: 1px solid #e8e8e8;
  position: relative;
  height: 6rem;
}
.sel-goods .have-sel img{
  display: block;
  position: absolute;
  width: 8rem;
  height: 8rem;
  left: 1rem;
  top:-3rem;
  border: 1px solid #e8e8e8;
  
}
.sel-goods .have-sel .sel-property{
  margin-left: 10rem;
}
.sel-goods .have-sel .sel-property .sel-price{
  color:#5B73f2;
  font-size: 1.4rem;
  margin-top: 1.2rem;
}
.sel-goods .have-sel .sel-property .have-sel-property{
  color:#666666;
  font-size: 0.8rem;
  margin-top: 0.5rem;
}
.sel-goods .close-sel{
  float:right;
  margin: 0.3rem 0.3rem 0 0 ;
  font-size: 1.8rem;
  z-index: 1000;
}
.goods-property{
  border-bottom: 1px solid #e8e8e8;
  overflow: hidden;
  padding: 1rem;
}
.goods-property .property-title{
  font-size: 1.2rem;
  margin-bottom: 0.5rem;
  color: black;
}
.goods-property .property-item{
  background: #f6f6f6;
  text-align: center;
  padding: 0.3rem;
  width: 5rem;
  float: left;
  font-size: 0.8rem;
  margin: 0.5rem;
}
.sel-goods .sel-mount{
  padding: 0.5rem;
  text-indent: 0.5rem;
  display: -webkit-flex;
  display:-moz-flex;
  display:-ms-flex;
  display:-o-flex;
  display:flex;
  justify-content:space-between;
  -ms-align-items: center;
  align-items: center;
  border-bottom: 1px solid #e8e8e8;
  padding: 0.5rem 1rem;
}
.sel-goods .sel-mount .sel-mount-operate{
  width: 120px;
}
.sel-goods .sel-mount .sel-mount-operate input{
  text-align: center;
}
.go-buy{
  padding: 2rem 0 1rem 0;
}
.go-buy .button{
  border: 1px solid #8D9CF7;
  color: #8D9CF7;
  margin: 0.5rem auto;
  border-radius: 2rem;
  width: 60%;
  height: 3rem;
  font-size: 1.4rem;
  line-height: 3rem;
  text-align: center;
  background: #5A72F1;
  color:white;
}
.have-sel-active{
  background: #FF5A5F !important;
  color: white;
}
.nont-goods{
  color: #ccc !important;
}