    }
    .msg{
      width: 100%;
      @include box-sizing(border-box);
      padding-top: 50*0.005+rem;
      padding-right: 30*0.005+rem;
      position: relative;
      font-size: 0.15rem;
      color:#222;
      line-height: 1.6;
      text-align: left;
      word-break:break-all;
    }
    .photo{
      width: 100%;
      @include box-sizing(border-box);
      padding-top: 30*0.005+rem;
      padding-right: 30*0.005+rem;
      ul.photo-ul{
        width: 100%;
        text-align: left;
      }
      li.photo-li{
        display: inline-block;
        width: 100*0.005+rem;
        height: 100*0.005+rem;
        margin-right:10*0.005+rem;
        margin-bottom:10*0.005+rem;
        position: relative;
        background-color: #f4f4f4;
        @include box-sizing(border-box);
        overflow: hidden;
        img{
          position: absolute;
          top:50%;
          left:50%;
          @include transform(translate3d(-50%,-50%,0));
        }
