    padding-bottom: 30*0.005+rem;
    @include box-sizing(border-box);
    padding-left: 100*0.005+rem;
    .avatar{
      width: 80*0.005+rem;
      height: 80*0.005+rem;
      border-radius: 50%;
      overflow: hidden;
      position: absolute;
      top:30*0.005+rem;
      left:0;
      background-color: #eee;
      @extend %useBackground;
      i{
        display: block;
