/* 消息样式 */
.messager{ font-size:12px; overflow: hidden; border:1px #e0e0e0 solid; position:fixed; _position:absolute; right:2px; bottom:2px; background:#fff; color:#666666;height:auto!important; }
.messager .msg_title{ overflow: hidden; height:40px;background-color:#f8f8f8; font-size:14px; padding:0 10px; border-bottom:1px #e0e0e0 solid; }
.messager .msg_title .msg_title_close{ float:right; width:16px; height:16px; cursor:pointer; overflow:hidden; background:url(close.gif) no-repeat; }
.messager .msg_title .msg_title_close:hover{ background-position:0 -16px; }
.messager .msg_content{ line-height:1.4; padding:10px; word-wrap: break-word;height:340px;overflow: auto; }

.messager .msg_content h2{
	margin-bottom:10px;
	/*padding:5px 0;*/
}
